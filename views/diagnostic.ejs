                                            <div class="custom-control custom-checkbox checkbox-lg">
                                                <input type="checkbox" name="consommation" id="consommationA" class="custom-control-input">
                                                <label class="custom-control-label ml-1 pl-2" for="consommationA">Junk
                                                    Food</label>
                                            </div>
                                            <div class="custom-control custom-checkbox checkbox-lg">
                                                <input type="checkbox" name="consommation" id="consommationB" class="custom-control-input" value="huile_de_tomate">
                                                <label class="custom-control-label ml-1 pl-2"
                                                    for="consommationB">Cigarette</label>
                                            </div>
                                            <div class="custom-control custom-checkbox checkbox-lg">
                                                <input type="checkbox" name="consommation" id="consommationC" class="custom-control-input">
                                                <label class="custom-control-label ml-1 pl-2"
                                                    for="consommationC">Café</label>
                                            </div>
                                            <div class="custom-control custom-checkbox checkbox-lg">
                                                <input type="checkbox" name="consommation" id="consommationD" class="custom-control-input">
                                                <label class="custom-control-label ml-1 pl-2"
                                                    for="consommationD">Alcool</label>
                                            </div>
                                            <div class="custom-control custom-checkbox checkbox-lg">
                                                <input type="checkbox" name="consommation" id="consommationE" class="custom-control-input">
                                                <label class="custom-control-label ml-1 pl-2" for="consommationE">Aucun
                                                    de ces
                                                    choix</label>
                                            </div>
                                    </fieldset>
