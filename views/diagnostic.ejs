                        <form role="form" action="/diagnostic/result" method="post">
                            <div class="tab-content">
                                <div class="tab-pane fade in active" role="tabpanel" id="stepper-step-1">
                                    <fieldset>
                                        <legend>Mon environnement est plutôt :</legend>
                                        <div class="form-group">
                                            <div class="custom-control custom-radio checkbox-lg" id="pollution">
                                                <input type="radio" name="pollution" id="pollutionA" class="custom-control-input" value="huile_de_framboise,huile_de_tomate">
                                                <label class="custom-control-label" for="pollutionA">Pollué,
                                                    je vie dans
                                                    une grande ville</label>
                                            </div>
                                            <div class="custom-control custom-radio checkbox-lg"  id="pollution">
                                                <input type="radio" name="pollution" id="pollutionB" class="custom-control-input">
                                                <label class="custom-control-label" for="pollutionB">Peu
                                                    pollué, je vie
                                                    dans une ville moyenne</label>
                                            </div>
                                            <div class="custom-control custom-radio checkbox-lg"  id="pollution">
                                                <input type="radio" name="pollution" id="PollutionC" class="custom-control-input">
                                                <label class="custom-control-label" for="PollutionC">Non
                                                    pollué, je vie à
                                                    la campagne</label>
                                            </div>
                                    </fieldset>
                                    <ul class="list-inline pull-right">
                                        <li>
                                            <a class="btn btn-dark next-step font-weight-bold">Suivant &#62;</a>
                                        </li>
                                    </ul>
                                </div>
                                <div class="tab-pane fade" role="tabpanel" id="stepper-step-2">
                                    <fieldset>
                                        <legend>Je consomme :</legend>
